<Defs>
  <TraitDef>
    <defName>GBKT_Taskmaster</defName>
    <commonality>1</commonality>
    <degreeDatas>
      <li>
        <label>Taskmaster</label>
        <description>{PAWN_nameDef} hates doing work themselves and prefers to assign burdensome tasks to others. Why do it yourself when you are skilled at leading others to do it for you?</description>
        <statFactors>
          <!-- Optional. Modifies pawn stats by multiplier -->
        </statFactors>
        <statOffsets>
          <WorkSpeedGlobal>-0.60</WorkSpeedGlobal>
          <MentalBreakThreshold>-0.08</MentalBreakThreshold>
        </statOffsets>
      </li>
    </degreeDatas>
    <conflictingTraits>
      <li>GBKT_CreativePlanner</li>
      <li>GBKT_MeleeCommander</li>
      <li>GBKT_Bodyguard</li>
      <li>GBKT_Teacher</li>
      <li>GBKT_Gibbering</li>
      <li>GBKT_ResearchAssistant</li>
      <li>GBKT_BeastMaster</li>
      <li>GBKT_Outrider</li>
      <li>GBKT_PlayLeader</li>
      <li>GBKT_CheerLeader</li>
      <li>GBKT_FireBrigadier</li>
